{}

let digit = ['0'-'9'];
let number = '0' | ['1'-'9'] digit*;

rule lex_is_number[T, S, P](lexbuf : @lexbuf.T[T, S, P]) -> Bool {
    parse {
        number => { true }
        _ => { false }
    }
}
{
pub fn is_number(str : String) -> Bool {
    let lexbuf = @lexbuf.from_string(str)
    lex_is_number(lexbuf)
}
}
